    function onInterval2() {
        currentItem2++;
        carouselContainerElement2.style.transitionDuration = "0.5s";
        carouselContainerElement2.style.transform = `translateX(-${currentItem2 * 100}%)`;

        if (currentItem2 === carouselItemElements2.length - 1) {
