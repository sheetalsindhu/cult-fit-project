            carouselContainerElement2.style.transitionDuration = "0s";
            carouselContainerElement2.style.transform = `translateX(0)`;
            clearInterval(interval2);
            currentItem2 = 0;
            interval2 = setInterval(onInterval2, 2000);
        }

        console.log(currentItem2);
    })





    // Modal login logout
    function create() {
        window.location.pathname = "main/profile/signUp.html";
    }

    function submit() {
        var mobilea = document.getElementById("mobile").value;

        var passworda = document.getElementById("password").value;

        var get = JSON.parse(localStorage.getItem("user"));
        // console.log(get);

        get.forEach(function (p) {
            if (p.number == mobilea && p.password == passworda) {
                document.getElementById("modal-body").innerHTML = "";

                var pa = document.createElement("p");
                pa.setAttribute("class", "login-alert");

                pa.innerHTML = "Login successful";
                pa.style.color = "green";
                //   p.style.font-size = "30px";

                let obj = {
                    n: p.number,
                    pass: p.password,
                };

                // Login text null after login
                document.getElementsByClassName("login-text").innerHTML = "";



                var dropd = document.getElementById("dropdown");
                dropd.style.display = "block";

                localStorage.setItem("logedin", JSON.stringify(obj));
                document.getElementById("modal-body").append(pa);
            }
        });
    }

    function call() {
        var modal = document.getElementById("modal");
        modal.style.display = "block";
    }
    function clos() {
        var modal = document.getElementById("modal");
        modal.style.display = "none";
    }




</script>